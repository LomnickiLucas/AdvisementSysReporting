      </Fields>
      <Query>
        <DataSourceName>ReportingDataSource</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT        program.programname AS Expr1, issue.issuename, issue.status, issue.urgency, campus.cname
FROM            issue INNER JOIN
                         student ON issue.studentid = student.studentid INNER JOIN
                         program ON student.programcode = program.programcode INNER JOIN
                         campus ON student.campus = campus.cname</CommandText>
        <Timeout>0</Timeout>
      </Query>
