      <Query>
        <DataSourceName>ReportingDataSource</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT        employee.employeeid, employee.fname, employee.lname, employee.phonenum, employee.email, employee.faculty, employee.role, issue.employeeid AS Expr1, 
                         issue.issueid, issue.issuename, issue.date, issue.status, issue.urgency
FROM            employee INNER JOIN
                         issue ON employee.employeeid = issue.employeeid</CommandText>
